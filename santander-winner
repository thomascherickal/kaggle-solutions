    "    etd[f + '_has_one'] = 0\n",
    "    etd[f + '_has_zero'] = 0\n",
    "    f_1 = data.loc[data[target] == 1, f].unique()\n",
    "    f_0 = data.loc[data[target] == 0, f].unique()\n",
    "    etd.loc[:, f + '_has_one'] = etd[f].isin(f_1).astype(int)\n",
    "    etd.loc[:, f + '_has_zero'] = etd[f].isin(f_0).astype(int)\n",
